{
  "name": "design-system",
  "type": "module",
  "main": "./nuxt.config.ts",
  "license": "ISC",
  "scripts": {
    "clean-node": "rm -rf node_modules && rm -rf ./packages/*/node_modules",
    "clean-nuxt": "rm -rf .nuxt && rm -rf ./packages/*/.nuxt",
    "clean-project": "pnpm clean-node && pnpm clean-nuxt && rm -rf ./packages/*/auto-imports.d.ts && rm -rf ./packages/*/components.d.ts",
    "dev": "nuxi dev",
    "postinstall": "nuxt prepare",
    "typecheck": "nuxt typecheck"
  },
  "packageManager": "pnpm@8.14.2",
  "devDependencies": {
    "@formkit/addons": "1.5.9",
    "@formkit/icons": "1.5.9",
    "@formkit/nuxt": "1.5.9",
    "@formkit/pro": "0.122.7",
    "@formkit/themes": "1.5.9",
    "@nuxt/eslint-config": "0.2.0",
    "@nuxt/image": "1.4.0",
    "@nuxtjs/i18n": "8.1.1 ",
    "@nuxtjs/tailwindcss": "6.11.4",
    "@types/node": "20.11.25",
    "@vueuse/components": "10.9.0",
    "@vueuse/nuxt": "10.9.0",
    "chromatic": "11.0.7",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-nuxt": "4.0.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-storybook": "0.8.0",
    "eslint": "8.57.0",
    "jszip": "3.10.1",
    "nuxt-headlessui": "1.1.5",
    "nuxt": "3.10.3",
    "prettier-plugin-tailwindcss": "0.5.12",
    "prettier": "3.2.5",
    "sass-loader": "14.1.1",
    "sass": "1.71.1",
    "tsx": "4.7.1",
    "type-fest": "4.12.0",
    "typescript": "5.4.2",
    "unplugin-auto-import": "0.17.5",
    "unplugin-icons": "0.18.5",
    "unplugin-vue-components": "0.26.0",
    "vite-tsconfig-paths": "4.3.1",
    "vue-tsc": "1.8.27"
  }
